<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Users.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Users"
    xmlns:pages="clr-namespace:Users.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="Users">
     <ShellContent
        Title="Dispositivos"
        ContentTemplate="{DataTemplate pages:DevicesPage}"
        Route="dispositivos" />
    <ShellContent
        Title="Usuarios"
        ContentTemplate="{DataTemplate pages:UsersPage}"
        Route="users" />

    

</Shell>
